{
    "name": "phpsimple/queue",
    "description": "Php Simple FIFO queue implementation",
    "type": "library",
    "keywords": [
        "php",
        "queue"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "simpletoimplement",
            "email": "simpletoimplement@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "spaghetti/php-cs-fixer-config": ">=1.1",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Phpsimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSimple\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-simple": "1.x-dev"
        },
        "thanks": {
            "name": "phpsimple/queue",
            "url": "https://github.com/phpsimple/queue"
        }
    },
    "funding": [
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/phpsimple/queue"
        }
    ]
}
